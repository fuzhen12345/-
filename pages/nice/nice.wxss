.nav {
  width: 100%;
  height: 400rpx;
}
image {
  width: 100%;
  height: 100%;
}
.bom {
  margin-top: 30rpx;
  display: flex;
  width: 100%;
  flex-wrap: wrap;
  justify-content: space-between;
}
.bom .boms {
  width: 48%;
  height: 360rpx;
  box-sizing: border-box;
  text-align: center;
  border-radius: 10px;
  margin-top: 30rpx;
  background-color: #daddda;
}
.bom .boms image {
  margin-top: 20rpx;
  width: 200rpx;
  height: 200rpx;
}
.bom .boms .n {
  margin-top: 20rpx;
}
